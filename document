hooks:
    global:
        __i_modular_nc_item_class_0 : آیتم های ساده
        __i_modular_c_item_class_0 : آیتم های پیچیده

    menu : i_modular_menu
        __main_link_href__ : لینک اصلی
        __logo_class__ : کلاس لوگو
        __login_register_class__ : کلاس دکمه لاگین,به لینک یا دکمه داده شود
        __login_register_class__2 :کلاس دکمه لاگین دوم,به لینک یا دکمه داده شود
        __mobile_class__ : کلاس شماره موبایل فوتر
        __phone_class__ : کلاس شماره تلفن فوتر
        __email_class__ : کلاس ایمیل
        __social_class__ : کلاس پرنت شبکه های اجتماعی
        __aboutUsLink__ :   لینک درباره ما
        __instagram_class__ :   کلاس اینستاگرام
        __whatsapp_class__ :    کلاس واتساپ
        __telegram_class__ :    کلاس تلگرام
