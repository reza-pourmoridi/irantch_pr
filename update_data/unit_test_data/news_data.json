\u0645\u0646\u062a\u0634\u0631 \u0634\u062f\u0647 \u0627\u0633\u062a\u060c \u06a9\u0634\u0648\u0631 \u062a\u0631\u06a9\u06cc\u0647 \u0627\u0632 \u0645\u0627\u0647 \u0698\u0627\u0646\u0648\u06cc\u0647 \u062a\u0627 \u0645\u0627\u0647 \u0633\u067e\u062a\u0627\u0645\u0628\u0631 \u0645\u06cc\u0632\u0628\u0627\u0646 \u06f4\u06f4 \u0645\u06cc\u0644\u06cc\u0648\u0646 \u0648 \u06f6\u06f0\u06f0 \u0647\u0632\u0627\u0631 \u06af\u0631\u062f\u0634\u06af\u0631 \u0628\u0648\u062f\u0647 \u06a9\u0647 \u0646\u0633\u0628\u062a \u0628\u0647 \u0632\u0645\u0627\u0646 \u0645\u0634\u0627\u0628\u0647 \u062f\u0631 \u0633\u0627\u0644 \u06af\u0630\u0634\u062a\u0647 \u06f1\u06f3.\u06f2 \u062f\u0631\u0635\u062f \u0631\u0634\u062f \u062f\u0627\u0634\u062a\u0647 \u0627\u0633\u062a.<\/p>\r\n\r\n<p>\u062f\u0631 \u0633\u0647 \u0645\u0627\u0647\u0647 \u0633\u0648\u0645 \u0633\u0627\u0644 \u062c\u0627\u0631\u06cc \u0645\u06cc\u0644\u0627\u062f\u06cc (\u062a\u0627\u0628\u0633\u062a\u0627\u0646 \u0627\u0645\u0633\u0627\u0644)\u060c \u062f\u0631\u0622\u0645\u062f \u06af\u0631\u062f\u0634\u06af\u0631\u06cc \u062a\u0631\u06a9\u06cc\u0647 \u0628\u0627 \u06f1\u06f3 \u062f\u0631\u0635\u062f \u0627\u0641\u0632\u0627\u06cc\u0634 \u0646\u0633\u0628\u062a \u0628\u0647 \u0645\u062f\u062a \u0645\u0634\u0627\u0628\u0647 \u0633\u0627\u0644 \u0642\u0628\u0644 \u0628\u0647 \u0628\u0627\u0644\u063a \u0628\u0631 \u06f2\u06f0 \u0645\u06cc\u0644\u06cc\u0627\u0631\u062f \u062f\u0644\u0627\u0631 \u0631\u0633\u06cc\u062f\u0647 \u0627\u0633\u062a. \u062f\u0627\u062f\u0647 \u0647\u0627\u06cc \u062a\u0631\u06a9&zwnj;\u0627\u0633\u062a\u0627\u062a \u0646\u0634\u0627\u0646 \u0645\u06cc&zwnj;\u062f\u0647\u062f \u06a9\u0647 \u062d\u062f\u0648\u062f \u06f1\u06f6.\u06f5 \u062f\u0631\u0635\u062f \u0627\u0632 \u062f\u0631\u0622\u0645\u062f \u06af\u0631\u062f\u0634\u06af\u0631\u06cc \u0646\u0627\u0634\u06cc \u0627\u0632 \u0628\u0627\u0632\u062f\u06cc\u062f \u0627\u062a\u0628\u0627\u0639 \u062a\u0631\u06a9 \u0645\u0642\u06cc\u0645 \u062e\u0627\u0631\u062c \u0627\u0632 \u0627\u06cc\u0646 \u06a9\u0634\u0648\u0631 \u0628\u0648\u062f\u0647 \u0627\u0633\u062a.<\/p>\r\n\r\n<p>\u062a\u0631\u06a9\u06cc\u0647 \u062a\u0646\u0647\u0627 \u062f\u0631 \u062a\u0627\u0628\u0633\u062a\u0627\u0646 \u0627\u0645\u0633\u0627\u0644 \u06f2\u06f2 \u0645\u06cc\u0644\u06cc\u0648\u0646 \u0648 \u06f4\u06f0\u06f0 \u0647\u0632\u0627\u0631 \u0628\u0627\u0632\u062f\u06cc\u062f\u06a9\u0646\u0646\u062f\u0647 \u062f\u0627\u0634\u062a\u0647 \u0627\u0633\u062a\u060c \u062f\u0631 \u062d\u0627\u0644\u06cc \u06a9\u0647 \u0627\u06cc\u0646 \u0631\u0642\u0645 \u062f\u0631\u062a\u0627\u0628\u0633\u062a\u0627\u0646 \u0633\u0627\u0644 \u06af\u0630\u0634\u062a\u0647 \u0628\u0647 \u06f2\u06f1 \u0645\u06cc\u0644\u06cc\u0648\u0646 \u0646\u0641\u0631 \u0631\u0633\u06cc\u062f\u0647 \u0628\u0648\u062f.<\/p>\r\n\r\n<p>\u0627\u0632 \u0627\u06cc\u0646 \u062a\u0639\u062f\u0627\u062f\u060c \u062d\u062f\u0648\u062f \u06f1\u06f2 \u062f\u0631\u0635\u062f \u06cc\u0627 \u06f2 \u0645\u06cc\u0644\u06cc\u0648\u0646 \u0648 \u06f7\u06f0\u06f0 \u0647\u0632\u0627\u0631 \u0646\u0641\u0631 \u0631\u0627 \u0634\u0647\u0631\u0648\u0646\u062f\u0627\u0646 \u062a\u0631\u06a9\u06cc\u0647&zwnj;\u0627\u06cc \u062a\u0634\u06a9\u06cc\u0644 \u062f\u0627\u062f\u0646\u062f \u06a9\u0647 \u0645\u0642\u06cc\u0645 \u06a9\u0634\u0648\u0631\u0647\u0627\u06cc \u062f\u06cc\u06af\u0631 \u0647\u0633\u062a\u0646\u062f \u0648 \u0628\u0631\u0627\u06cc \u0633\u0641\u0631 \u0628\u0647 \u0648\u0637\u0646 \u062e\u0648\u062f \u0631\u0641\u062a\u0647 \u0628\u0648\u062f\u0646\u062f.<\/p>\r\n\r\n<p>\u062f\u0631 \u062a\u0627\u0628\u0633\u062a\u0627\u0646 \u0627\u0645\u0633\u0627\u0644\u060c \u0645\u06cc\u0627\u0646\u06af\u06cc\u0646 \u0647\u0632\u06cc\u0646\u0647 \u0647\u0631 \u0634\u0628 \u0627\u0642\u0627\u0645\u062a \u062f\u0631 \u062a\u0631\u06a9\u06cc\u0647 \u0628\u0631\u0627\u06cc \u0647\u0631 \u0641\u0631\u062f&nbsp; \u0645\u0639\u0627\u062f\u0644 \u06f1\u06f0\u06f3 \u062f\u0644\u0627\u0631 \u0628\u0648\u062f\u0647 \u0627\u0633\u062a\u060c \u0648 \u0645\u06cc\u0627\u0646\u06af\u06cc\u0646 \u0647\u0632\u06cc\u0646\u0647&zwnj;\u06a9\u0631\u062f \u0628\u0627\u0632\u062f\u06cc\u062f\u06a9\u0646\u0646\u062f\u06af\u0627\u0646 \u062f\u0631 \u06a9\u0644 \u0645\u062f\u062a \u0627\u0642\u0627\u0645\u062a&zwnj;\u0634\u0627\u0646 \u062f\u0631 \u062a\u0631\u06a9\u06cc\u0647 \u0646\u06cc\u0632 \u0645\u0639\u0627\u062f\u0644 \u06f9\u06f0\u06f2 \u062f\u0644\u0627\u0631 \u0628\u0647 \u0627\u0632\u0627\u06cc \u0647\u0631 \u0646\u0641\u0631 \u0627\u0646\u062f\u0627\u0632\u0647&zwnj;\u06af\u06cc\u0631\u06cc \u0634\u062f\u0647 \u0627\u0633\u062a.<\/p>\r\n\r\n<p>\u062f\u0631 \u062d\u0627\u0644\u06cc&zwnj;\u06a9\u0647 \u0647\u0632\u06cc\u0646\u0647&zwnj;\u0647\u0627\u06cc \u0632\u0646\u062f\u06af\u06cc \u062f\u0631 \u062a\u0631\u06a9\u06cc\u0647 \u0627\u0632 \u062c\u0645\u0644\u0647 \u0648\u0631\u0632\u0634\u060c \u0622\u0645\u0648\u0632\u0634 \u0648 \u0641\u0631\u0647\u0646\u06af \u06f3\u06f3 \u062f\u0631\u0635\u062f\u060c \u0647\u0632\u06cc\u0646\u0647 \u0645\u0633\u06a9\u0646 \u06f2\u06f6 \u062f\u0631\u0635\u062f \u0648 \u0647\u0632\u06cc\u0646\u0647 \u063a\u0630\u0627 \u0648 \u0646\u0648\u0634\u06cc\u062f\u0646\u06cc \u06f2\u06f4 \u062f\u0631\u0635\u062f \u0627\u0641\u0632\u0627\u06cc\u0634 \u06cc\u0627\u0641\u062a\u0647 \u0627\u0633\u062a\u060c \u0627\u0645\u0627 \u0628\u0627 \u0627\u06cc\u0646 \u062d\u0627\u0644 \u0647\u0632\u06cc\u0646\u0647&zwnj;\u0647\u0627\u06cc \u062e\u062f\u0645\u0627\u062a \u062a\u0648\u0631\u0647\u0627\u06cc \u0645\u0633\u0627\u0641\u0631\u062a\u06cc \u062f\u0631 \u062a\u0627\u0628\u0633\u062a\u0627\u0646 \u0627\u0645\u0633\u0627\u0644 \u06f1.\u06f6 \u062f\u0631\u0635\u062f \u0646\u0633\u0628\u062a \u0628\u0647 \u0645\u062f\u062a \u0645\u0634\u0627\u0628\u0647 \u0633\u0627\u0644 \u0642\u0628\u0644 \u06a9\u0627\u0647\u0634 \u062f\u0627\u0634\u062a\u0647 \u0627\u0633\u062a.<\/p>\r\n<\/div>\r\n","feature_image":"articles\/55517866255517-1658.png","feature_